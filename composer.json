{
    "name": "cego/service-client-base",
    "description": "A package containing utility and abstractions for various specific clients implementations",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Niki Ewald Zakariassen",
            "email": "niza@cego.dk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "vlucas/phpdotenv": "^5.2",
        "phpoption/phpoption": "^1.7",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.0",
        "cego/request-insurance": "^0.1.3",
        "orchestra/testbench": "^6.13"
    },
    "suggest": {
        "cego/request-insurance": "Allows for sending asynchronous requests"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
